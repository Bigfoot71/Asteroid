#include <winver.h>

#define COMPANY_NAME         "Bigfoot71"
#define FILE_DESCRIPTION     "Asteroid - Game made for raylib Slo-Jam 2023-2024"
#define FILE_VERSION         1,0,0,0
#define FILE_VERSION_STR     "1.0.0.0"
#define PRODUCT_NAME         "Asteroid"
#define PRODUCT_VERSION      1,0,0,0
#define PRODUCT_VERSION_STR  "1.0.0.0"
#define ORIGINAL_FILENAME    "Asteroid.exe"
#define INTERNAL_NAME        "Asteroid"
#define LEGAL_COPYRIGHT      "Copyright (C) 2024 Le Juez Victor"
#define IDI_ICON1             101

VS_VERSION_INFO VERSIONINFO
 FILEVERSION FILE_VERSION
 PRODUCTVERSION PRODUCT_VERSION
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS VOS__WINDOWS32
 FILETYPE VFT_APP
 FILESUBTYPE VFT2_UNKNOWN
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904E4"
    BEGIN
      VALUE "CompanyName", COMPANY_NAME
      VALUE "FileDescription", FILE_DESCRIPTION
      VALUE "FileVersion", FILE_VERSION_STR
      VALUE "InternalName", INTERNAL_NAME
      VALUE "OriginalFilename", ORIGINAL_FILENAME
      VALUE "ProductName", PRODUCT_NAME
      VALUE "ProductVersion", PRODUCT_VERSION_STR
      VALUE "LegalCopyright", LEGAL_COPYRIGHT
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x409, 1252
  END
END

IDI_ICON1               ICON    "icon.ico"
